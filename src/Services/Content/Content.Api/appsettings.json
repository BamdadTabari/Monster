{
  "ConnectionStrings": {
    "Content": "Host=localhost;Port=5432;Database=monster_content;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": { "Microsoft": "Warning", "System": "Warning" }
    },
    "Enrich": [ "FromLogContext", "WithThreadId", "WithProcessId", "WithMachineName" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
